{
  "bracketSpacing": false,
  "singleQuote": false,
  "semi": true,
  "useTabs": false,
  "tabWidth": 2,
  "trailingComma": "all",
  "printWidth": 120,
  "arrowParens": "always",
  "importOrder": ["react", "^(?!react)\\w+$", "^[./]"],
  "importOrderParserPlugins": ["typescript", "tsx", "javascript", "jsx"],
  "importOrderSeparation": false,
  "importOrderSortSpecifiers": true
}
